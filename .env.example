APP_NAME="OBSOLIO"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Centralized Logging
LOKI_ENABLED=false
LOKI_URL=http://localhost:3100
LOKI_PUSH_URL=http://localhost:3100/loki/api/v1/push

# Database Configuration (PostgreSQL)
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=obsolio_db
DB_USERNAME=postgres
DB_PASSWORD=

# Database Query Monitoring
DB_QUERY_MONITORING_ENABLED=false
DB_SLOW_QUERY_THRESHOLD=1000
DB_LOG_ALL_QUERIES=false
DB_LOG_QUERY_TOTALS=true
DB_MAX_QUERIES_PER_REQUEST=50
DB_PG_STAT_STATEMENTS_ENABLED=true

# Connection Pooling (PgBouncer)
PGBOUNCER_ENABLED=false
PGBOUNCER_HOST=127.0.0.1
PGBOUNCER_PORT=6432
PGBOUNCER_POOL_MODE=transaction
PGBOUNCER_MAX_CLIENT_CONN=1000
PGBOUNCER_DEFAULT_POOL_SIZE=25
PGBOUNCER_MIN_POOL_SIZE=10

# Session Configuration (Database)
# Use 'database' for file-based sessions without Redis
# Change to 'redis' if you have Redis installed and running
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local

# Queue Configuration (Sync)
# Use 'sync' to process jobs immediately without Redis
# Change to 'redis' if you have Redis installed for better performance
QUEUE_CONNECTION=sync

# Cache Configuration (File)
# Use 'file' for filesystem caching without Redis
# Change to 'redis' if you have Redis installed for better performance
CACHE_STORE=file
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

# Redis Configuration
REDIS_CLIENT=predis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_DB=1

# Mail Configuration
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Tenancy Configuration
TENANCY_DATABASE_AUTO_DELETE=false
TENANCY_DATABASE_AUTO_DELETE_USER=false

# Sanctum Configuration
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1

# JWT Authentication
JWT_SECRET=
JWT_TTL=60
JWT_REFRESH_TTL=20160

# Frontend Configuration
FRONTEND_URL=http://localhost:5173

# CORS Configuration
# Use * for all origins (development only) or specify comma-separated origins
# Example: http://localhost:3000,http://localhost:5173
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# CDN Configuration
CDN_ENABLED=false
CDN_PROVIDER=cloudflare
CDN_ASSET_VERSION=1.0.0

# CloudFront Configuration
CDN_CLOUDFRONT_URL=
CDN_CLOUDFRONT_DISTRIBUTION_ID=
CDN_CLOUDFRONT_KEY_PAIR_ID=
CDN_CLOUDFRONT_PRIVATE_KEY_PATH=

# Cloudflare CDN Configuration
CDN_CLOUDFLARE_URL=
CDN_CLOUDFLARE_ZONE_ID=
CDN_CLOUDFLARE_API_TOKEN=
CDN_CLOUDFLARE_TOKEN_SECRET=

# Fastly Configuration
CDN_FASTLY_URL=
CDN_FASTLY_SERVICE_ID=
CDN_FASTLY_API_KEY=

# Bunny CDN Configuration
CDN_BUNNY_URL=
CDN_BUNNY_PULL_ZONE_ID=
CDN_BUNNY_API_KEY=
CDN_BUNNY_STORAGE_ZONE=
CDN_BUNNY_TOKEN_SECRET=

# CDN Cache Settings
CDN_PURGE_ON_DEPLOY=true
CDN_PRELOAD_ENABLED=true
CDN_IMAGE_OPTIMIZATION=true
CDN_IMAGE_QUALITY=85
CDN_CORS_ORIGINS=*

# Compression Configuration
COMPRESSION_ENABLED=true
COMPRESSION_GZIP_ENABLED=true
COMPRESSION_GZIP_LEVEL=6
COMPRESSION_BROTLI_ENABLED=true
COMPRESSION_BROTLI_LEVEL=4
COMPRESSION_MIN_SIZE=1024
COMPRESSION_PRECOMPRESS=true
COMPRESSION_CACHE_ENABLED=false
COMPRESSION_LOGGING_ENABLED=false

VITE_APP_NAME="${APP_NAME}"
