              "mode": "raw",
              "raw": "{\n  \"name\": \"Invoice Processor\",\n  \"description\": \"Processes incoming invoices\",\n  \"type\": \"custom\",\n  \"engines_used\": [],\n  \"config\": {},\n  \"status\": \"draft\"\n}",
              "options": {
