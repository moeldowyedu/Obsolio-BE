          }
        },
        {
          "name": "Create Organization",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"New Organization\",\n  \"industry\": \"Technology\",\n  \"company_size\": \"50-200\",\n  \"country\": \"USA\",\n  \"timezone\": \"America/New_York\"\n}",
              "options": {
